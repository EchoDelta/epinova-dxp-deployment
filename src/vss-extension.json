{
    "manifestVersion": 1,
    "id": "episerver-dxp-deploy-extension",
    "publisher": "epinova-sweden",
    "version": "1.0.0",
    "name": "Episerver DXP API Deploy (BETA)",
    "description": "Tasks to deploy package to Episerver DXP with DXP API.",
    "public": false,
    "icons":{
        "default": "img/Epinova_e_128x128.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "tags":[
        "Release",
        "Episerver",
        "Deploy",
        "DXC"
    ],
    "branding":{
        "color": "rgb(246,247,251)",
        "theme": "light"
    },
    "content": {
        "details": {
            "path": "overview.md"
        },
        "license": {
            "path": "license.md"
        }
    },
    "links": {
        "company": {
            "uri": "https://www.epinova.se"
        }
    },
    "contributions": [
        {
            "id": "dxp-deploynugetpackage-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "DeployNuGetPackage"
            }
        },
        {
            "id": "dxp-smoketestiffailreset-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "SmokeTestIfFailReset"
            }
        },
        {
            "id": "dxp-completedeploy-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "CompleteDeploy"
            }
        },
        {
            "id": "dxp-movedeploy-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "MoveDeploy"
            }
        },
        {
            "id": "dxp-resetdeploy-buildtask",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "ResetDeploy"
            }
        }
    ],
    "files": [
        {
            "path": "DeployNuGetPackage"
        },
        {
            "path": "SmokeTestIfFailReset"
        },
        {
            "path": "CompleteDeploy"
        },
        {
            "path": "MoveDeploy"
        },
        {
            "path": "ResetDeploy"
        },
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}